{
  "name": "claws",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build-dev-dependencies && pm2-runtime ecosystem.config.js",
    "pm2-docker-dev": "npm run build-dev-dependencies && pm2-runtime ecosystem.config.js --env development",
    "pm2-local-prod": "pm2 start ./ecosystem.config.js",
    "pm2-local-dev": "npm run build-dev-dependencies && pm2 start ./ecosystem.config.js --env development",
    "stop": "pm2 stop ./ecosystem.config.js",
    "reload": "pm2 reload ./ecosystem.config.js",
    "status": "pm2 list",
    "flush": "pm2 flush logs",
    "delete": "pm2 delete all",
    "build-dev-dependencies": "browserify ./public/salsa20.js -o ./public/salsa20.min.js",
    "dev": "npm run build-dev-dependencies && cross-env NODE_ENV=development nodemon --inspect server.js",
    "local-prod": "npm run build-dev-dependencies && cross-env NODE_ENV=production nodemon server.js"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "browserify": "^16.2.3",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "ffprobe-static": "^3.0.0",
    "js-salsa20": "^1.0.0",
    "jsonwebtoken": "^8.4.0",
    "m3u8-stream-list": "^1.1.0",
    "memorystore": "^1.6.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "os-utils": "0.0.14",
    "pm2": "^3.4.0",
    "random-useragent": "^0.3.1",
    "rate-limiter-flexible": "^0.19.6",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "url": "^0.11.0",
    "winston": "^3.2.1",
    "ws": "^6.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.14.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
